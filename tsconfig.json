{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "dom"],
    // "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
